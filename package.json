{
  "name": "wisp",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "npm --workspace client run start",
    "start:server": "npm --workspace server run start"
  },
  "dependencies": {
    "concurrently": "^9.0.1"
  }
}
